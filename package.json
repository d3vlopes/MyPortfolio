{
  "name": "my-portfolio",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typecheck": "tsc --project tsconfig.json --noEmit",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema-temp.gql --tagName=gql --outputFlat src/graphql/generate",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "postinstall": "husky install",
    "prepare": "husky install"
  },
  "husky": {
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@styled-icons/bootstrap": "^10.34.0",
    "@styled-icons/boxicons-logos": "^10.37.0",
    "@styled-icons/boxicons-regular": "^10.38.0",
    "@styled-icons/boxicons-solid": "^10.38.0",
    "@styled-icons/entypo-social": "^10.34.0",
    "@styled-icons/evaicons-solid": "^10.34.0",
    "@styled-icons/fluentui-system-filled": "^10.35.0",
    "@styled-icons/foundation": "^10.34.0",
    "@styled-icons/material-outlined": "^10.34.0",
    "@styled-icons/remix-fill": "^10.18.0",
    "dotenv": "^10.0.0",
    "graphql": "^15.5.1",
    "medium-zoom": "^1.0.6",
    "next": "11.1.0",
    "next-pwa": "^5.2.24",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-inlinesvg": "^2.3.0",
    "styled-components": "5.3.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@storybook/addon-essentials": "6.3.7",
    "@storybook/react": "6.3.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.6",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.1",
    "@types/react": "^17.0.18",
    "@types/styled-components": "^5.1.12",
    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "apollo": "^2.33.4",
    "babel-loader": "^8.2.2",
    "babel-plugin-styled-components": "^1.13.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.1",
    "jest": "^27.0.6",
    "jest-styled-components": "^7.0.5",
    "lint-staged": "^11.0.0",
    "plop": "^2.7.4",
    "prettier": "^2.3.2",
    "storybook-addon-designs": "^6.1.0",
    "storybook-addon-next-router": "^3.0.7",
    "typescript": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
